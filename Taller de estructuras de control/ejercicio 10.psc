Proceso inicio_cambio_divisas
	Escribir "Ingrese el valor en chelines"
	Leer ch
	pe<-(ch*956.871)/100
	Escribir pe " pesetas"
	Escribir "Ingrese el valor en dragmas griegos"
	Leer dr
	pe<-(dr*88.607)/100
	ff<-pe/20.110
	Escribir ff " francos franceses"
	Escribir "Ingrese el valor en cpesetas"
	Leer pe
	uss<-pe/122.499
	li<-(pe*100)/9.289
	Escribir uss " dolares " li " lirio italiano"  
FinProceso
