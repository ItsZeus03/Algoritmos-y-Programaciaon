Proceso inicio_velocidad
	Escribir "Ingrese la velocidad del carro 1 en km/h"
	Leer v1
	Escribir "Ingrese la velocidad del carro 2 en km/h"
	Leer v2
	Escribir "Ingrese la distancia entre los carros en m"
	Leer d
	a<-d/(v2-v1)
	s<-a*60
	Escribir "el tiempo que se demora el carro 2 en alcansar el carro 1 es de " s " min"
FinProceso
